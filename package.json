{
  "name": "random-bible",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/lukemoderwell/random-bible.git",
  "author": "lukemoderwell <luke.moderwell@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "sass": "node-sass --watch ./src/scss -o ./dist/css",
    "watch": "parcel watch ./src/index.html",
    "build": "rm -rf ./dist && parcel build ./src/index.html",
    "start": "parcel build ./src/index.html && node ./src/server.js"
  },
  "dependencies": {
    "airtable": "^0.5.2",
    "axios": "^0.18.0",
    "express": "^4.16.2",
    "node-sass": "^4.7.2"
  },
  "now": {
    "env": {
      "AIRTABLE_API_KEY": "@airtable_api_key",
      "AIRTABLE_BASE": "@airtable_base",
      "TOTAL_ENTRIES": "@total_entries"
    }
  },
  "devDependencies": {
    "parceljs": "^0.0.1"
  }
}
